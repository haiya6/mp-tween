{
  "compilerOptions": {
    "target": "ES3",
    "module": "ESNext",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "include": [
    "src/**/*.ts"
  ]
}
